{
  "name": "@ameshkin/nextcrumbs",
  "version": "0.2.0",
  "description": "MUI 7 breadcrumbs with Next.js Link support, optional URL auto-generation, and SEO microdata.",
  "license": "MIT",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "author": "Amir Meshkin <amir.meshkin@gmail.com> (https://github.com/ameshkin)",
  "main": "./public/index.js",
  "module": "./public/index.js",
  "types": "./public/index.d.ts",
  "exports": {
    ".": {
      "types": "./public/index.d.ts",
      "import": "./public/index.js",
      "default": "./public/index.js"
    },
    "./breadcrumbs": {
      "types": "./public/Breadcrumbs.d.ts",
      "import": "./public/Breadcrumbs.js"
    },
    "./hooks/usePathBreadcrumbs": {
      "types": "./public/hooks/usePathBreadcrumbs.d.ts",
      "import": "./public/hooks/usePathBreadcrumbs.js"
    },
    "./hooks/useReactRouterBreadcrumbs": {
      "types": "./public/hooks/useReactRouterBreadcrumbs.d.ts",
      "import": "./public/hooks/useReactRouterBreadcrumbs.js"
    },
    "./utils/jsonld": {
      "types": "./public/utils/jsonld.d.ts",
      "import": "./public/utils/jsonld.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "tsup --clean",
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e:ui": "playwright test",
    "test:e2e:bdd": "cucumber-js \"tests/e2e/features/**/*.feature\" --require \"tests/e2e/steps/**/*.mjs\"",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "prepublishOnly": "npm run typecheck && npm run test:run && npm run build",
    "publish:all": "npm run typecheck && npm run test:run && npm run build && npm version patch -m \"chore(release): %s\" && git push --follow-tags && npm publish && npm publish --access public --registry https://registry.npmjs.org/",
    "orchestrator:ci": "ts-node ./scripts/orchestrator-ci.ts",
    "clean-build": "bash ./scripts/orchestrator-clean-build.sh",
    "orchestrator:clean-build": "bash ./scripts/orchestrator-clean-build.sh",
    "jira:projects": "node scripts/jira-projects.js"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "@mui/icons-material": ">=7",
    "@mui/material": ">=7",
    "react": ">=18",
    "react-dom": ">=18",
    "react-router-dom": ">=6 || >=7"
  },
  "peerDependenciesMeta": {
    "@emotion/react": {
      "optional": false
    },
    "@emotion/styled": {
      "optional": false
    },
    "react-router-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ameshkin/jira-mate": "^0.1.17",
    "@cucumber/cucumber": "^11.2.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "jsdom": "^22.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.6",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ameshkin/nextcrumbs.git"
  },
  "bugs": {
    "url": "https://github.com/ameshkin/nextcrumbs/issues"
  },
  "homepage": "https://github.com/ameshkin/nextcrumbs#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ameshkin"
  },
  "engines": {
    "node": ">=20.19 <21 || >=22.12"
  },
  "packageManager": "npm@10.5.0",
  "keywords": [
    "mui",
    "breadcrumbs",
    "nextjs",
    "react",
    "material-ui",
    "seo",
    "schema.org",
    "app router",
    "react router"
  ],
  "publishConfig": {
    "access": "public"
  }
}
