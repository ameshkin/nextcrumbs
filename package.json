{
  "name": "@ameshkin/nextcrumbs",
  "version": "0.0.4",
  "description": "MUI 7 breadcrumbs with Next.js Link support, optional URL auto-generation, and SEO microdata.",
  "license": "MIT",
  "type": "module",
  "author": "Amir Meshkin <amir.meshkin@gmail.com> (https://github.com/ameshkin)",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release:pre": "npm run typecheck && npm run test && npm run build",
    "release:patch": "npm run release:pre && npm version patch -m \"chore(release): %s\" && git push && git push --tags && npm publish --access public",
    "publish:patch": "npm run release:patch"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "@mui/icons-material": ">=7",
    "@mui/material": ">=7",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/jest-dom": "^6.4.2",
    "vitest": "^4.0.5",
    "jsdom": "^22.1.0",
    "@vitejs/plugin-react": "^5.1.0",
    "typescript": "^5.9.3",
    "tsup": "^8.5.0"
  },
  "peerDependenciesMeta": {
    "@emotion/react": {
      "optional": false
    },
    "@emotion/styled": {
      "optional": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ameshkin/nextcrumbs.git"
  },
  "bugs": {
    "url": "https://github.com/ameshkin/nextcrumbs/issues"
  },
  "homepage": "https://github.com/ameshkin/nextcrumbs#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ameshkin"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mui",
    "breadcrumbs",
    "nextjs",
    "react",
    "material-ui",
    "seo",
    "schema.org",
    "app router"
  ],
  "publishConfig": {
    "access": "public"
  }
}
